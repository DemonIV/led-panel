<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest Version="7.0" ExtensionBundleId="com.ledpanel.aeplugin" 
  ExtensionBundleVersion="1.0.0" ExtensionBundleName="LED Panel Manager">
  <Author>LED Panel Team</Author>
  <Contact>support@ledpanel.com</Contact>
  <Abstract>LED Panel Management for After Effects</Abstract>
  <ExtensionList>
    <Extension Id="com.ledpanel.aeplugin.panel" Version="1.0.0"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="AEFT" Version="[17.0,99.9]"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="7.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.ledpanel.aeplugin.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/ae-scripts.jsx</ScriptPath>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>LED Panel Manager</Menu>
          <Geometry>
            <Size>
              <Height>600</Height>
              <Width>400</Width>
            </Size>
            <MinSize>
              <Height>400</Height>
              <Width>300</Width>
            </MinSize>
          </Geometry>
          <Icons>
            <Icon Type="Normal">./assets/icon-normal.png</Icon>
            <Icon Type="RollOver">./assets/icon-hover.png</Icon>
            <Icon Type="Disabled">./assets/icon-disabled.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>